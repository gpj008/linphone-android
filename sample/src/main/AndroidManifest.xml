<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.longrise.jie.sample">

    <uses-permission android:name="READ_PHONE_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Default">

        <activity android:name="com.longrise.jie.sample.LinphoneMiniLoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.longrise.jie.sample.LinphoneMiniActivity"/>

        <service android:name="com.longrise.jie.sample.LinphoneMiniService"/>

    </application>

</manifest>
